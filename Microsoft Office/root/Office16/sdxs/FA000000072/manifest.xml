        <Authorization>
          <Resource>https://outlook.com/</Resource>
          <Scopes>
            <Scope>Notes.Read</Scope>
            <Scope>Notes.ReadWrite</Scope>
          </Scopes>
        </Authorization>
      </Authorizations>
    </WebApplicationInfo>
